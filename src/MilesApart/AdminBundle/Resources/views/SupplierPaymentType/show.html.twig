{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/finance.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'supplier_payment_type' %}

{# Set the page title #}
{% block title %}
    {{ entity.supplierpaymenttypename }} - Supplier Payment Type
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.supplierpaymenttypename }} - Supplier Payment Type           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Supplier Payment Type details
{% endblock %}

{% block page_content -%}

    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Supplier payment type name</th>
                <td>{{ entity.supplierpaymenttypename }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('finance_supplier-payment-type') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('finance_supplier-payment-type_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
