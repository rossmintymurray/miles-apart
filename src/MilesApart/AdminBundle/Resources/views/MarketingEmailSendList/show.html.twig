{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/campaign.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'marketing_email_send_list' %}

{# Set the page title #}
{% block title %}
    {{ entity.id }} - Marketing Email Send List
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.id }} - Marketing Email Send List           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Marketing Email Send List details
{% endblock %}

{% block page_content -%}


    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Marketing email send list date created</th>
                <td>{{ entity.marketingemailsendlistdatecreated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Marketing email send list date modified</th>
                <td>{{ entity.marketingemailsendlistdatemodified|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('campaign_marketing-email-send-list') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('campaign_marketing-email-send-list_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
