{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/campaign.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'traffic_source' %}

{# Set the page title #}
{% block title %}
    {{ entity.trafficsourcecompanyname }} - Traffic Source
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.trafficsourcecompanyname }} - Traffic Source           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Traffic Source details
{% endblock %}

{% block page_content -%}

    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Traffic source company name</th>
                <td>{{ entity.trafficsourcecompanyname }}</td>
            </tr>
            <tr>
                <th>Traffic source company url</th>
                <td>{{ entity.trafficsourcecompanyurl }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('campaign_traffic-source') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('campaign_traffic-source_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
