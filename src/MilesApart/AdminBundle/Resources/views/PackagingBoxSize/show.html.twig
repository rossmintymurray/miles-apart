{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/customerorder.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'packaging_box_size' %}

{# Set the page title #}
{% block title %}
    - Packaging Box Size
{% endblock%}

{# Set the page header #}
{% block page_name %}
    - Packaging Box Size           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Packaging Box Size details
{% endblock %}

{% block page_content -%}

    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Packagin box size width</th>
                <td>{{ entity.packagingboxsizewidth }}</td>
            </tr>
            <tr>
                <th>Packaging box size height</th>
                <td>{{ entity.packagingboxsizeheight }}</td>
            </tr>
            <tr>
                <th>Packaging box size depth</th>
                <td>{{ entity.packagingboxsizedepth }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('customer-order_packaging-box-size') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('customer-order_packaging-box-size_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
