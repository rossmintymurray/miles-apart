{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/product.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'attribute_value' %}

{# Set the page title #}
{% block title %}
    {{ entity.attributevalue }} - Attribute Value
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.attributevalue }} - Attribute Value        
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View attribute value details
{% endblock %}

{% block page_content -%}

    <table class="table table-hover table-bordered table-striped main_content_table show_table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Attribute value</th>
                <td>{{ entity.attributevalue }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('product_attribute-value') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('product_attribute-value_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
