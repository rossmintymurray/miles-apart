{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/business.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'stocktake_product' %}

{# Set the page title #}
{% block title %}
    {{ entity.id }} - Stocktake Product
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.id }} - Stocktake Product           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Stocktake Product details
{% endblock %}

{% block page_content -%}

    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Stocktake product qty</th>
                <td>{{ entity.stocktakeproductqty }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('business_stocktake-product') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('business_stocktake-product_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
