{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/finance.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'daily_take' %}

{# Set the page title #}
{% block title %}
     {{ entity.dailytakedate|date('Y-m-d H:i:s') }} - Daily Take
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.dailytakedate|date('Y-m-d H:i:s') }} - Daily Take           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Daily Take details
{% endblock %}

{% block page_content -%}


    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Daily take date</th>
                <td>{{ entity.dailytakedate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('finance_daily-take') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('finance_daily-take_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
