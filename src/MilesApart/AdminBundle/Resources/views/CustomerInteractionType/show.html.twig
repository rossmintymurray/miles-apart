{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/hr.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'customer_interaction_type' %}

{# Set the page title #}
{% block title %}
   {{ entity.customerinteractiontypename }}  - Customer Interaction Type
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.customerinteractiontypename }} - Customer Interaction Type           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Customer Interaction Type details
{% endblock %}

{% block page_content -%}


    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Customer interaction type name</th>
                <td>{{ entity.customerinteractiontypename }}</td>
            </tr>
            <tr>
                <th>Customer interaction type description</th>
                <td>{{ entity.customerinteractiontypedescription }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('customer_customer-interaction-type') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('customer_customer-interaction-type_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
