{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/campaign.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'email_send_list_customer' %}

{# Set the page title #}
{% block title %}
    {{ entity.id }} - Email Send List Customer
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.id }} - Email Send List Customer           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Email Send List Customer details
{% endblock %}

{% block page_content -%}


    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Email send list customer date added</th>
                <td>{{ entity.emailsendlistcustomerdateadded|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('campaign_email-send-list-customer') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('campaign_email-send-list-customer_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
