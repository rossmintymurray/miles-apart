{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/category.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'category_type' %}

{# Set the page title #}
{% block title %}
    {{ entity.categorytype }} - Category Type
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.categorytype }} - Category Type          
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View category type details
{% endblock %}

{% block page_content -%}

    <table class="table table-hover table-bordered table-striped main_content_table show_table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Category type</th>
                <td>{{ entity.categorytype }}</td>
            </tr>
            <tr>
                <th>Category type table name</th>
                <td>{{ entity.categorytypetablename }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('category_category-type') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('category_category-type_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
