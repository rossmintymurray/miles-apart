{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/product.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'product_cost' %}

{# Set the page title #}
{% block title %}
    {{ entity.id }} - Product Cost
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.id }} - Product Cost           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Product Cost details
{% endblock %}

{% block page_content -%}

    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Product cost valid from</th>
                <td>{{ entity.productcostvalidfrom|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Product cost valid until</th>
                <td>{{ entity.productcostvaliduntil|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Product cost value</th>
                <td>{{ entity.productcostvalue }}</td>
            </tr>
            <tr>
                <th>Product cost is special</th>
                <td>{{ entity.productcostisspecial }}</td>
            </tr>
            <tr>
                <th>Product cost date created</th>
                <td>{{ entity.productcostdatecreated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Product cost date modified</th>
                <td>{{ entity.productcostdatemodified|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('product_product-cost') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('product_product-cost_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
