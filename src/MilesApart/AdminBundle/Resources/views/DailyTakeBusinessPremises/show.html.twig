{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/finance.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'daily_take_business_premises' %}

{# Set the page title #}
{% block title %}
    - Daily Take Business Premises
{% endblock%}

{# Set the page header #}
{% block page_name %}
    - Daily Take Business Premises           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Daily Take Business Premises details
{% endblock %}

{% block page_content -%}


    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Z reading cash</th>
                <td>{{ entity.zreadingcash }}</td>
            </tr>
            <tr>
                <th>Z reading card</th>
                <td>{{ entity.zreadingcard }}</td>
            </tr>
            <tr>
                <th>Counted cash</th>
                <td>{{ entity.countedcash }}</td>
            </tr>
            <tr>
                <th>Counted card</th>
                <td>{{ entity.countedcard }}</td>
            </tr>
            <tr>
                <th>Daily take business premises date created</th>
                <td>{{ entity.dailytakebusinesspremisesdatecreated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Daily take business premises date modified</th>
                <td>{{ entity.dailytakebusinesspremisesdatemodified|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('finance_daily-take-business-premises') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('finance_daily-take-business-premises_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
