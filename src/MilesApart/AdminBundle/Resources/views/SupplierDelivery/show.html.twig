{# src/MilesApart/AdminBundle/Resources/views/Employees/show.html.twig #}
{% extends 'MilesApartAdminBundle::Page/procurement.html.twig' %}

{# Set  current page for interface highlighting #}
{% set current_page = 'supplier_delivery' %}

{# Set the page title #}
{% block title %}
    {{ entity.id }} - Supplier Delivery
{% endblock%}

{# Set the page header #}
{% block page_name %}
    {{ entity.id }} - Supplier Delivery           
{% endblock %}   

{# Set the page sub header #}
{% block page_introduction %}
    View Supplier Delivery details
{% endblock %}

{% block page_content -%}

    <table class=\"table table-hover table-bordered table-striped main_content_table show_table\">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Booked in datetime</th>
                <td>{{ entity.bookedindatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Contact name</th>
                <td>{{ entity.contactname }}</td>
            </tr>
            <tr>
                <th>Contact phone number</th>
                <td>{{ entity.contactphonenumber }}</td>
            </tr>
            <tr>
                <th>Delivered datetime</th>
                <td>{{ entity.delivereddatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Supplier delivery date created</th>
                <td>{{ entity.supplierdeliverydatecreated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Supplier delivery date modified</th>
                <td>{{ entity.supplierdeliverydatemodified|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('goods-in_supplier-delivery') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('goods-in_supplier-delivery_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
